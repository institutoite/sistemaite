use App\Models\Estudiante;
use App\Models\Persona;
use Carbon\Carbon;

  
  $nuevos=Persona::join("estudiantes",'estudiantes.persona_id','personas.id')
		->join("inscripciones",'inscripciones.estudiante_id','estudiantes.id')
		->join("programacions",'programacions.inscripcione_id','inscripciones.id')
		->join('clases','clases.programacion_id','programacions.id')
		->where("clases.fecha",'=','2024-03-28')
		->select('personas.id',"nombre",'apellidop','clases.horainicio','horafin')
	  	->orderBy('clases.horainicio','asc')
		->groupBy('personas.id') 
		->get();

use App\Models\Estudiante;
use App\Models\Persona;
use Carbon\Carbon;

  
  $nuevos=Persona::join("estudiantes",'estudiantes.persona_id','personas.id')
		->join("inscripciones",'inscripciones.estudiante_id','estudiantes.id')
		->join("programacions",'programacions.inscripcione_id','inscripciones.id')
		->join('clases','clases.programacion_id','programacions.id')
		->where("clases.fecha",'=','2024-03-28')
		->select('personas.id',"nombre",'apellidop','clases.horainicio','horafin')
	->groupBy('personas.id')  	
	->orderBy('clases.horainicio','asc')
		 
		->get();

use App\Models\Estudiante;
use App\Models\Persona;
use Carbon\Carbon;

  
  $nuevos=Persona::join("estudiantes",'estudiantes.persona_id','personas.id')
		->join("inscripciones",'inscripciones.estudiante_id','estudiantes.id')
		->join("programacions",'programacions.inscripcione_id','inscripciones.id')
		->join('clases','clases.programacion_id','programacions.id')
		->where("clases.fecha",'=','2024-03-28')
		->select('personas.id',"nombre",'apellidop','clases.horainicio','horafin')
	->groupBy('personas.id',"nombre",'apellidop','clases.horainicio','horafin')  	
	->orderBy('clases.horainicio','asc')
		 
		->get();

