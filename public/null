use Illuminate\Support\Facades\Http;
use App\Http\Controllers\ComentarioController;

$response = Http::post('http://localhost/api/guardar-comentario', [
    'nombre' => 'Juan Pérez',
    'telefono' => '1234567890',
    'comentario' => 'Este es un comentario de prueba.',
  	'como_id' => 6,
    'recaptcha_token' => 'TOKEN_DE_RECAPTCHA',
]);


dd($response->json());
