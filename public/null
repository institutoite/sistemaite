use App\Models\Persona;
use Carbon\Carbon;
use Illuminate\Support\Facades\Storage;
use Intervention\Image\Facades\Image;

$persona=Persona::findOrFail(1);
$observaciones=$persona->observaciones;
        
$gestiones=$persona->estudiante->grados;
$nota="%%%%%%%%%%%%%  GRADOS Y GESTIONES %%%%%%%%%%%%%\n";
foreach ($gestiones as $grado) {
  $nota=$nota.$grado->grado." - ".$grado->pivot->anio."\n";
}

$nota.="%%%%%%%%%%%%%  OBSERVACIONES %%%%%%%%%%%%%\n";;
foreach ($observaciones as $value) {
  $nota=$nota.$value->observacion."(".$value->created_at.")\n";
}

$nota.="%%%%%%%%%%%%% INSCRIPCIONES %%%%%%%%%%%%%\n";;
$inscripciones=$persona->estudiante->inscripciones;
foreach ($inscripciones as $inscripcion) {
  $nota="Modalidad:".$nota.$inscripcion->modalidad->modalidad." - Costo:".$inscripcion->costo."Estado:".$inscripcion->estado->estado."\n";
}

$nota.="%%%%%%%%%%%%% MATRICULACIONES %%%%%%%%%%%%%\n";;
$matriculaciones=$persona->computacion->matriculaciones;
foreach ($matriculaciones as $matriculacion) {
  $nota="Asignatura:".$nota.$matriculacion->asignatura->asignatura." - Costo:".$matriculacion->costo."Estado:".$matriculacion->estado->estado."\n";
}

$nota.="%%%%%%%%%%%%% APODERADOS %%%%%%%%%%%%%\n";;
$apoderados=$persona->apoderados;
foreach ($apoderados as $apoderado) {
  $nota="Apoderado:".$apoderado->nombre.$apoderado->apellidop." - Parentesco:".$apoderado->pivot->parentesco."teléfono:".$apoderado->telefono."\n";

}
$nota;
